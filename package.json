{
  "name": "simple-travel-service",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app.ts",
  "scripts": {
    "dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "dotenv_config_path=dev && nodemon --watch \"dist/**/*\" -e js ./dist/app.js",
    "build:test": "dotenv_config_path=test && swc src -d dist",
    "build:prod": "dotenv_config_path=prod && swc src -d dist",
    "lint:fix": "eslint '*/**/*.{js,ts}' --fix",
    "lint": "eslint '*/**/*.{js,ts}'",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abiyogaaron/simple-travel-service.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abiyogaaron/simple-travel-service/issues"
  },
  "homepage": "https://github.com/abiyogaaron/simple-travel-service#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.49",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "chokidar": "^3.5.3",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  }
}
